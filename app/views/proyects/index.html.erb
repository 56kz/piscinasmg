<h1>Proyectos</h1>


<table>
  <tr>
    <th>Id</th>
    <th>Nombre</th>
    <th>Link De Youtube</th>
    <th>Primer link</th>
  </tr>
  <% @proyects.each do |p| %>
    <tr>
      <td><%= p.id %></td>
      <td><%= p.name %></td>
      <td><%= p.youtube %></td>
      <td><%= p.linkone %></td>
    </tr>
  <% end %>
</table>
